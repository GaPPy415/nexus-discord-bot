{
  "name": "nexus-discord-bot",
  "version": "1.0.0",
  "description": "Nexus Discord Bot",
  "author": "Delemangi",
  "license": "MIT",
  "homepage": "https://github.com/Delemangi/nexus-discord-bot",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Delemangi/nexus-discord-bot.git"
  },
  "bugs": {
    "url": "https://github.com/Delemangi/nexus-discord-bot/issues"
  },
  "main": "src/index.ts",
  "type": "module",
  "engines": {
    "node": "^24"
  },
  "scripts": {
    "build": "npm run clean && node esbuild.config.js && tsc-alias",
    "check": "tsc --noEmit",
    "clean": "rimraf dist",
    "dev": "tsx src/index.ts",
    "format": "eslint . --cache --fix",
    "lint": "eslint . --cache",
    "start": "node dist/index.js",
    "start:env": "node --env-file=.env dist/index.js"
  },
  "dependencies": {
    "@sapphire/framework": "^5.5.0",
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3",
    "winston-transport": "^4.9.0"
  },
  "devDependencies": {
    "esbuild": "^0.27.2",
    "eslint": "^9.39.2",
    "eslint-config-imperium": "^3.2.0",
    "rimraf": "^6.1.2",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3"
  }
}
